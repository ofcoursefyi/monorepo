{
  "name": "ofc",
  "private": "true",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "turbo": "^1.12.4"
  },
  "scripts": {
    "db:studio": "turbo db:studio --filter @ofc/schema",
